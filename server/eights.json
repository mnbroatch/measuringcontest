{
  "DEBUG_DISABLE_SECRET_STATE": true,
  "entities": [
    {
      "type": "Space",
      "name": "hand",
      "perPlayer": true,
      "contentsHiddenFrom": "Others"
    },
    {
      "type": "Space",
      "name": "discard"
    },
    {
